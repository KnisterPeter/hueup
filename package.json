    "css-loader": "3.2.0",
    "file-loader": "4.2.0",
    "html-webpack-plugin": "3.2.0",
    "prettier": "1.18.2",
    "rimraf": "3.0.0",
    "style-loader": "1.0.0",
    "ts-loader": "6.1.0",
    "typescript": "3.6.2",
    "webpack": "4.39.3",
    "webpack-bundle-analyzer": "3.4.1",
    "webpack-cli": "3.3.8",
    "webpack-dev-server": "3.8.0",
    "webpack-pwa-manifest": "4.0.0",
    "workbox-webpack-plugin": "4.3.1"
  },
  "dependencies": {
    "@material-ui/core": "^4.4.0",
    "@material-ui/icons": "^4.2.1",
    "mobx": "^5.13.0",
    "mobx-react": "^6.1.3",
    "preact": "next",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "styled-components": "^4.3.2",
    "tslib": "^1.10.0",
    "typeface-roboto": "^0.0.75"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "prettier": {},
